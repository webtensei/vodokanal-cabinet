{
  "name": "vodokanal-cabinet",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --mode production",
    "build:test": "tsc && vite build --mode test",
    "build:docker": "tsc && vite build --mode docker",
    "preview": "vite preview",
    "lint": "tsc && eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"./**/*.{html,tsx,ts,js,json,md}\"",
    "prepare": "husky install",
    "docker:build": "tsc && docker build -t docker-react .",
    "docker:run": "docker rm -f docker-react && docker run --name docker-react -d -p 8082:8082 docker-react",
    "release:local": "yarn docker:build && yarn docker:run"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@nextui-org/react": "^2.2.9",
    "@nextui-org/theme": "^2.1.17",
    "@tanstack/react-query": "^5.28.6",
    "axios": "^1.6.2",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.8",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.51.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.19.0",
    "react-toastify": "^10.0.5",
    "tailwind-merge": "^2.2.2",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@tanstack/react-query-devtools": "^5.28.6",
    "@types/node": "^20.9.3",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.16",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.18",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.35",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.2",
    "vconsole": "^3.15.1",
    "vite": "^5.0.0"
  }
}
